{
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "arrowParens": "always",
        "bracketSameLine": false,
        "bracketSpacing": true,
        "jsxSingleQuote": true,
        "printWidth": 80,
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "all",
        "useTabs": false
      }
    }
  ]
}
